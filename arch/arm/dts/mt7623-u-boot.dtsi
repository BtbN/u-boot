/ {
	timer0: timer@10008000 {
		compatible = "mediatek,timer";
		reg = <0 0x10008000 0 0x80>;
		interrupts = <GIC_SPI 112 IRQ_TYPE_LEVEL_LOW>;
		clocks = <&system_clk>;
		clock-names = "system-clk";
		bootph-all;
	};
};

&topckgen {
	bootph-all;
};

&topckgen {
	bootph-all;
};

&pericfg {
	bootph-all;
};

&apmixedsys {
	bootph-all;
};

&uart2 {
	bootph-all;
};
